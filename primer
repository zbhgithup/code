int main()
{
	int i,j;
	int count = 0;
	for(i=2; i<=100; i++)
	{
	
		for(j=2; j<i; j++)
		{
			if((i%j)==0)
			{
				break;
			}
		}
		if(j==i)
		{
			count++;
			printf("%d ",i);
			
	
		}
	}
	printf("\n%d",count);
	return 0;
}
